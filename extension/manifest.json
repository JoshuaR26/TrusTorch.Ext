{
    "manifest_version": 2,
    "name": "Travel Popup",
    "version": "1.0",
    "description": "Shows a popup when the user searches for something travel related",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": [
      "tabs",
      "webNavigation",
      "https://www.google.com/*"
    ]
  }
  